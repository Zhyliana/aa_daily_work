
  <h1>
    <% if session_user.nil? %>
       Welcome cat-lover
    <%else%>
       Welcome, <%= session_user.user_name %>
     <%end%>
  </h1>

  <h2>Session new controller</h2>

  <form action="http://localhost:3000/users" method="POST">
    <input name="authenticity_token"
           type="hidden"
           value="<%= form_authenticity_token %>">
  <label for="new_username">Username</label>
  <input name="user[user_name]" type="text" id="new_username"></input>
  <br>
  <label for="new_password">Password</label>
  <input name="user[password]" type="password" id="new_password"></input>
  <br>
  <input type="submit" value="Sign In!"></input>

  </form>
